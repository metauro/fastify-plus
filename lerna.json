{
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "create": {
      "homepage": "https://github.com/metauro/fastify-plus",
      "license": "MIT"
    },
    "version": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "exact": true,
      "gitRemote": "master",
      "message": "chore(release): %s"
    }
  },
  "version": "0.0.1",
  "ignoreChanges": ["packages/**/test", "**/*.md"],
  "packages": ["packages/*"]
}
